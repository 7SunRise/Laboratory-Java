<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.test.storetransactionsystem.TransactionCreateController">
   <children>
      <HBox layoutX="1.0" layoutY="3.0">
         <children>
            <VBox>
               <children>
                  <Label text="Product:"/>
                  <ComboBox fx:id="ProductsComboBox"/>
				  <Label text="Seller:"/>
                  <ComboBox fx:id="SellersComboBox"/>
                  <Label text="Date"/>
                  <TextArea fx:id="DateTextArea"/>
               </children>
               <children>
                  <Button fx:id="okButton" onAction="#onOkButtonClick" text="Ok" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>